    "request": "launch",
    "useBundler": true,
    "cwd": "${workspaceRoot}/TestLibrary", // pod 命令执行的路径
    "program": "${workspaceRoot}/CocoaPods/bin/pod",
    "args": ["install", "--verbose"], // `pod` 命令的参数
    }
  ]
}

